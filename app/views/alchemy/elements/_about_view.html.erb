<%- cache(element) do -%>
  <%= element_view_for(element) do |el| -%>
    <!-- About -->
    <section id="about">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading text-uppercase"><%= el.render :title %></h2>
            <h3 class="section-subheading text-muted"><%= el.render :description %></h3>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12">
            <ul class="timeline">
              <% element.nested_elements.available.each do |nested_element| %>
                <%= render_element(nested_element) %>
              <% end %>
              <li class="timeline-inverted">
                <div class="timeline-image">
                  <h4>Be Part
                    <br>Of Our
                    <br>Story!</h4>
                </div>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </section>
  <%- end -%>
<%- end -%>
